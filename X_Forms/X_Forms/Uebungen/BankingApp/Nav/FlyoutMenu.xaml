<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="X_Forms.Uebungen.BankingApp.Nav.FlyoutMenu"
             xmlns:nav="clr-namespace:X_Forms.Uebungen.BankingApp.Nav"
            xmlns:v="clr-namespace:X_Forms.Uebungen.BankingApp.View">
  <FlyoutPage.Flyout>
    <nav:FlyoutMenuFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <v:BankingView/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>